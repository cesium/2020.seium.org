{
  "name": "seium.org",
  "description": "Semana da Engenharia Inform√°tica da Universidade do Minho",
  "version": "0.1.0-2021",
  "private": true,
  "scripts": {
    "dev": "npm-run-all develop",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --fix --ignore-path .gitignore .",
    "lint:css": "stylelint --fix --ignore-path .gitignore './**/*.{css,scss,sass}'",
    "fmt": "npm-run-all format",
    "format": "prettier --ignore-path .gitignore --write './**/*.{css,js,json,md,mdx}'",
    "test": "npm-run-all test:*",
    "test:lint": "npm-run-all test:lint:*",
    "test:lint:js": "eslint --ignore-path .gitignore .",
    "test:lint:css": "stylelint --ignore-path .gitignore './**/*.{css,scss,sass}'",
    "test:format": "prettier --ignore-path .gitignore --check './**/*.{css,js,json,md,mdx}'"
  },
  "dependencies": {
    "next": "^10.0.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-nextjs": "^1.0.6",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0"
  }
}
